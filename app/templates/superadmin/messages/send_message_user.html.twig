{% extends 'superadmin/superadmin_base.html.twig' %}

{% block content_title %} Message {% endblock %}

{% block body_content %}
	<div>
		{{ form_start(form) }}
			<div class="row">
				<div class="col-lg-12">
					{{ form_row(form.content, {'attr' : {'class' : 'd-none message-content'}}) }}
					<div app-editor></div>
				</div>
			</div>
		{{ form_rest(form) }}
		{{ form_end(form) }}
	</div>

{% endblock %}
